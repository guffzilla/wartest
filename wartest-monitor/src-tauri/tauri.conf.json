{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "WCArena Monitor",
  "version": "0.1.0",
  "build": {
    "frontendDist": "../dist",
    "beforeBuildCommand": ""
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "fullscreen": false,
        "resizable": true,
        "title": "WCArena Core",
        "width": 1000,
        "height": 800,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "identifier": "com.wcarena.monitor",
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}